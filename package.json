{
    "name": "@lvchengbin/url",
    "version": "0.3.0",
    "description": "Polyfill of URL and URLSearchParams for browsers.",
    "keywords": [
        "URL",
        "URLSearchParams",
        "polyfill"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:LvChengbin/url.git"
    },
    "devDependencies": {
        "buble": "^0.19.8",
        "eslint": "^6.6.0",
        "jasmine-core": "^3.5.0",
        "karma": "^6.3.14",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^2.0.1",
        "karma-rollup-preprocessor": "^7.0.2",
        "karma-server": "0.0.5",
        "optimist": "^0.6.1",
        "pre-commit": "^1.2.2",
        "rollup": "^1.25.2",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-node-resolve": "^5.2.0"
    },
    "scripts": {
        "build": "rollup -c",
        "tw" : "karma start",
        "test": "karma start --single-run",
        "test-es5": "karma start --es5=true",
        "prepublish": "npm run build",
        "lint": "eslint ./ --cache --ignore-path .eslintignore",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "pre-commit": [
        "precommit-msg",
        "lint"
    ],
    "main": "dist/url.cjs.js",
    "jsnext:main": "src/index.js",
    "author": "LvChengbin",
    "license": "MIT",
    "dependencies": {
        "@lvchengbin/is": "0.0.27",
        "puppeteer": "^2.0.0"
    }
}
